{
  "extends": "./tsconfig.json",
  "exclude": [
    "src/**/*.test.ts"
  ],
  "typedocOptions": {
    "entryPoints": [
      "./src/index.ts"
    ],
    "out": "./docs",
    "theme": "default",
    "plugin": "typedoc-plugin-markdown",
    "readme": "none",
    "githubPages": false,
    "disableSources": true,
    "excludeExternals": true,
    "excludePrivate": true,
    "includeVersion": true,
    "hideGenerator": true
  }
}
